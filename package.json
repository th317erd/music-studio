{
  "name": "music-studio",
  "version": "0.0.1",
  "description": "Music Studio for the browser",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build-client": "node ./initialize-before-run.js && ./node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=production node ./node_modules/webpack/bin/webpack.js --verbose --config=./client/config/webpack.config.prod.js",
    "build-client-test": "node ./initialize-before-run.js && ./node_modules/.bin/cross-env NODE_ENV=test BABEL_ENV=test node ./node_modules/webpack/bin/webpack.js --verbose --config=./client/config/webpack.config.test.js",
    "build-stand-alone": "node ./initialize-before-run.js && ./node_modules/.bin/cross-env NODE_ENV=stand-alone BABEL_ENV=stand-alone node ./node_modules/webpack/bin/webpack.js --verbose --config=./client/config/webpack.config.stand-alone.js",
    "initialize": "node ./initialize-before-run.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./",
    "rebuild-icons": "node tools/rebuild-icons.js",
    "start-client": "node ./initialize-before-run.js && ./node_modules/.bin/cross-env NODE_ENV=development BABEL_ENV=development ./node_modules/.bin/webpack serve --inline --config=./client/config/webpack.config.dev.js",
    "start-stand-alone": "npm run build-stand-alone && node ./node_modules/.bin/electron client/stand-alone",
    "start-server": "node ./initialize-before-run.js && node --inspect ./server/index.js",
    "test-client": "node ./node_modules/jest/bin/jest.js --config ./jest.config.client.js",
    "test-server": "node ./node_modules/jest/bin/jest.js --config ./jest.config.server.js",
    "test": "npm run test-client && npm run test-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/th317erd/music-studio"
  },
  "author": "Wyatt Greenway",
  "license": "MIT",
  "dependencies": {
    "@react-ameliorate/core": "^0.11.0",
    "autoprefixer": "10.2.3",
    "base-64": "^1.0.0",
    "better-sqlite3": "^7.1.2",
    "blueimp-md5": "^2.18.0",
    "browserify": "^17.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "color": "^3.1.3",
    "csv": "^5.3.2",
    "electron": "^11.2.1",
    "events": "^3.2.0",
    "evisit-js-utils": "^0.8.6",
    "extract-text-webpack-plugin": "3.0.2",
    "ffmpeg.js": "^4.2.9003",
    "file-loader": "6.2.0",
    "gsap": "^3.6.0",
    "hapi": "^17.8.1",
    "html-webpack-plugin": "4.5.1",
    "janap": "^1.0.6",
    "jquery": "^3.5.1",
    "jszip": "^3.5.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.2.0",
    "raf": "3.4.1",
    "re-reselect": "^4.0.0",
